[*.cs]

# CS8618: Il campo non nullable deve contenere un valore non Null all'uscita dal costruttore. Provare a dichiararlo come nullable.
dotnet_diagnostic.CS8618.severity = silent

# CS8601: Possibile assegnazione di riferimento Null.
dotnet_diagnostic.CS8601.severity = silent

# IDE0019: Usa i criteri di ricerca
dotnet_diagnostic.IDE0019.severity = none

# CS8603: Possibile restituzione di riferimento Null.
dotnet_diagnostic.CS8603.severity = none

# CS8629: Il tipo valore nullable non può essere Null.
dotnet_diagnostic.CS8629.severity = none

# Default severity for analyzer diagnostics with category 'Globalization'
dotnet_analyzer_diagnostic.category-Globalization.severity = none

# CS8604: Possibile argomento di riferimento Null.
dotnet_diagnostic.CS8604.severity = none
